  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main Graph",
      "type": "debugpy",
      "request": "launch",
      "module": "app.agents.main.graph_main",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug Backtest",
      "type": "debugpy",
      "request": "launch",
      "module": "app.util.ft.ft_backtesting",
      "console": "integratedTerminal"
    },
    {
      "name": "Python Debugger: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "fastapi",
      "args": ["dev"],
      "jinja": true,
      "justMyCode": false
    }
  ]
}
